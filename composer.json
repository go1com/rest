{
    "name":              "go1/rest",
    "license":           "MIT",
    "prefer-stable":     true,
    "minimum-stability": "dev",
    "autoload":          {
        "psr-4": {
            "go1\\rest\\": "./"
        },
        "files": [
            "helper.php"
        ]
    },
    "require":           {
        "php":                           "^7.4",
        "ext-json":                      "*",
        "firebase/php-jwt":              "^5.0",
        "nyholm/psr7":                   "^1.1.0",
        "opis/closure":                  "^3.5.6",
        "php-di/slim-bridge":            "2.0.0",
        "php-http/message":              "^1.7.2",
        "psr/http-client":               "^1.0.0",
        "psr/simple-cache":              "^1.0.1",
        "slim/slim":                     "^3.12.0",
        "symfony/console":               "^v4.2.3",
        "symfony/http-client":           "^v4.4.5",
        "symfony/http-client-contracts": "^1.1.8",
        "symfony/var-dumper":            "^v4.3.2",
        "datadog/dd-trace":              "^0.74.0"
    },
    "require-dev":       {
        "beberlei/assert":             "^3.2.0",
        "doctrine/dbal":               "^2.9.2",
        "elasticsearch/elasticsearch": "^5.4.0",
        "phpunit/phpunit":             "^8.0",
        "symfony/yaml":                "^v4.2.3",
        "symfony/cache":               "^v4.2.4",
        "justinrainbow/json-schema":   "^5.2.8"
    },
    "suggest":           {
        "php-amqplib/php-amqplib": "^2.6.0"
    },
    "bin":               [
        "rest-cli.php"
    ],
    "extra":             {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
